{
  "name": "fe_task-nexo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -e development craco start",
    "start:demo": "env-cmd -e demo craco start",
    "build": "env-cmd -e production craco build",
    "test": "craco test --env=jsdom --coverage=true --watchAll=false",
    "commit": "cz",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.ts,.tsx src --color",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx src --color",
    "format": "prettier --write src/**/*.{js,ts,tsx,scss,css,json,mdx}",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "npm run test",
    "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
    "release": "HUSKY=0 standard-version -a",
    "release:major": "npm run release -- --release-as major --no-verify",
    "release:minor": "npm run release -- --release-as minor --no-verify",
    "release:patch": "npm run release -- --release-as patch --no-verify",
    "initial-release": "npm run release -- --first-release --no-verify"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@craco/craco": "^7.0.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "antd": "^5.2.3",
    "axios": "^1.3.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "react-scripts": "^5.0.0",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "autoprefixer": "^10.4.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "tailwindcss": "^3.0.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run test"
    }
  }
}
